{
  "name": "@nuxt-drizzle/module",
  "version": "0.0.33",
  "description": "Nuxt module for automating Drizzle ORM setup with conditional database driver imports to prevent unused dependencies in the final bundle.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Myshkouski/nuxt-drizzle-module"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist/"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "prepare": "nuxt-module-build prepare",
    "prepack": "nuxt-module-build prepare && nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "nuxt-module-build build --stub && nuxt-module-build prepare && vitest run",
    "test:e2e": "nuxt-module-build build --stub && nuxt-module-build prepare && vitest run test/e2e/**",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264",
  "dependencies": {
    "@nuxt-drizzle/utils": "^0.0.14",
    "@nuxt/kit": "^4.3.1",
    "consola": "^3.4.2",
    "db0": "^0.3.4",
    "jiti": "^2.6.1",
    "nitropack": "^2.13.1",
    "ohash": "^2.0.11",
    "strip-indent": "^4.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260210.0",
    "@electric-sql/pglite": "^0.3.15",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.15.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.1",
    "@nuxt/test-utils": "^3.23.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.2.3",
    "changelogen": "^0.6.2",
    "drizzle-kit": "^0.31.9",
    "drizzle-orm": "^0.45.1",
    "eslint": "^9.39.2",
    "nuxt": "^4.3.1",
    "typescript": "^5.9.3",
    "unstorage": "^1.17.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.4"
  },
  "peerDependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "better-sqlite3": "^12.6.2",
    "h3": "^1.15.5",
    "mysql2": "^3.16.2",
    "postgres": "^3.4.8"
  },
  "peerDependenciesMeta": {
    "@electric-sql/pglite": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "postgres": {
      "optional": true
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "tar@<=7.5.2": ">=7.5.3",
      "tar@<=7.5.3": ">=7.5.4",
      "tar@<7.5.7": ">=7.5.7",
      "@isaacs/brace-expansion@<=5.0.0": ">=5.0.1",
      "axios@<=1.13.4": ">=1.13.5"
    }
  }
}
